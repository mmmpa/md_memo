{
  "globals": {
    "$": true,
    "ENV": true,
    "CONFIGURATION": true,
    "APPLICATION_META_INFORMATION": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "max-len": 0,
    "comma-dangle": 2,
    "no-cond-assign": 0,
    "no-confusing-arrow": 0,
    "no-constant-condition": 2,
    "semi": [2, "never"],
    "space-before-function-paren": [
      "error",
      "always"
    ],
    "block-scoped-var": 2,
    "curly": [
      2,
      "all"
    ],
    "default-case": 2,
    "no-div-regex": 2,
    "no-else-return": 2,
    "no-param-reassign": 0,
    "no-eq-null": 2,
    "no-floating-decimal": 2,
    "no-multi-spaces": [
      2,
      {
        "exceptions": {
          "Property": true,
          "ImportDeclaration": true,
          "VariableDeclarator": true,
          "AssignmentExpression": true
        }
      }
    ],
    "no-self-compare": 2,
    "wrap-iife": [
      2,
      "inside"
    ],
    "complexity": [
      1,
      100
    ],
    "no-return-assign": 0,
    "react/no-danger": 0,
    "dot-notation": 1,
    "guard-for-in": 1,
    "no-extend-native": 1,
    "no-iterator": 1,
    "no-console": 0,
    "no-loop-func": 1,
    "no-multi-str": 1,
    "no-process-env": 1,
    "no-proto": 1,
    "no-throw-literal": 1,
    "no-unused-expressions": 0,
    "radix": 1,
    "no-alert": 0,
    "no-extra-bind": 0,
    "yoda": 0,
    "react/jsx-filename-extension": 0,
    "react/prop-types": 0,
    "react/no-multi-comp": 0,
    "class-methods-use-this": 0,
    "jsx-a11y/href-no-hash": "off",
    "jsx-a11y/anchor-is-valid": ["warn", { "aspects": ["invalidHref"] }],
    "jsx-a11y/label-has-for": 0
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "airbnb"
  ]
}